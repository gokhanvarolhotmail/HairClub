		public void Main()
		{
			int taskResult = (int)ScriptResults.Success;
			bool fireAgain = true;

			using (ConnectionManager cm = Dts.Connections["IFS FTP"])
			{
				FtpClientConnection ftp = new FtpClientConnection(cm.AcquireConnection(null));

				try
				{
					ftp.ChunkSize = 1000;
					ftp.UsePassiveMode = true;

					string ImportFolder = Dts.Variables["ImportFolder"].Value.ToString();
					string FTPInRemoteFolder = Dts.Variables["FTPInRemoteFolder"].Value.ToString();
