            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using System.IO;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;


namespace ST_88d09b1101e340d493456ccef9f7873b.csproj
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion
        public void Main()
        {
            using (ConnectionManager cm = Dts.Connections["FTP Connection Manager"])
            {
                // FTP Connection from the SSIS Connection Manager
                //ConnectionManager cm = Dts.Connections["FTP Connection Manager"];
                FtpClientConnection ftp = new FtpClientConnection(cm.AcquireConnection(null));
                //cm.Properties["ServerName"].SetValue(cm, Dts.Variables["FTPServerName"].Value.ToString());
                //cm.Properties["ServerUserName"].SetValue(cm, Dts.Variables["FTPServerUserName"].Value.ToString());
                //cm.Properties["ServerPassword"].SetValue(cm, Dts.Variables["FTPServerPassword"].Value.ToString());
                string LocalXMLFilePath = Dts.Variables["LocalXMLFilePath"].Value.ToString();
                string RemoteXMLFTPPath = Dts.Variables["RemoteXMLFTPPath"].Value.ToString();
                string[] RemoteFolders;
                string[] RemoteFiles;

                try
                {
                    ftp.Connect();
                    ftp.SetWorkingDirectory(RemoteXMLFTPPath);
                    ftp.GetListing(out RemoteFolders, out RemoteFiles);
                    if (RemoteFiles != null && RemoteFiles.Length > 0)
                    {
                        ftp.ReceiveFiles(RemoteFiles, LocalXMLFilePath, true, true);
                        ftp.DeleteFiles(RemoteFiles);
                    }

                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                catch (Exception)
                {
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
                finally
                {
                    ftp.Close();
                }
            }
        }
    }
}]]></ProjectItem>
