        /// Initialise default instance of <see cref="ZipConstants">ZipConstants</see>
        /// </summary>
        /// <remarks>
        /// Private to prevent instances being created.
        /// </remarks>
        ZipConstants()
        {
            // Do nothing
        }
    }
